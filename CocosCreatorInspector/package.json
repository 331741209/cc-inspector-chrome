    "css-loader": "^0.28.7",
    "element-ui": "^2.0.11",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.19.0",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^2.9.1"
  }
